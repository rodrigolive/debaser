{
  "name": "@rodrigolive/debaser",
  "version": "1.0.0",
  "description": "Database migration tool that moves data between databases while anonymizing sensitive information",
  "main": "dist/index.js",
  "bin": {
    "debaser": "dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "ts-node src/cli.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "prepare": "npm run build"
  },
  "keywords": [
    "database",
    "migration",
    "anonymization",
    "mysql",
    "postgresql",
    "sqlite",
    "streaming"
  ],
  "author": "rodrigolive",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/rodrigolive/debaser.git"
  },
  "homepage": "https://github.com/rodrigolive/debaser#readme",
  "bugs": {
    "url": "https://github.com/rodrigolive/debaser/issues"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "dependencies": {
    "chalk": "^4.1.2",
    "commander": "^11.1.0",
    "debaser": "file:debaser-1.0.0.tgz",
    "mysql2": "^3.6.5",
    "ora": "^5.4.1",
    "pg": "^8.11.3",
    "sqlite3": "^5.1.6",
    "yaml": "^2.3.4"
  },
  "devDependencies": {
    "@types/jest": "^29.5.8",
    "@types/node": "^20.10.5",
    "@types/pg": "^8.10.9",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.3"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "directories": {
    "example": "examples"
  },
  "types": "./dist/index.d.ts",
  "publishConfig": { "access": "restricted" }
}
